<!-- saving this form template in case we go back to it later.
was originally called in new.html.erb as partial form -->
<%= form_for @user do |f| %>
  <%= f.label :username %>:
  <%= f.text_field :username %>

  <%= f.label :password %>:
  <%= f.password_field :password %>

  <%= f.label :email %>:
  <%= f.text_field :email %>

  <%= f.submit %>
<% end %>
